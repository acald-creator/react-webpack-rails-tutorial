{
  "name": "react-webpack-rails-tutorial",
  "version": "1.1.0",
  "description": "Code from the React Webpack tutorial.",
  "engines": {
    "node": "12",
    "yarn": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shakacode/react-webpack-rails-tutorial.git"
  },
  "author": "justin808",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shakacode/react-webpack-rails-tutorial/issues"
  },
  "homepage": "https://github.com/shakacode/react-webpack-rails-tutorial",
  "scripts": {
    "lint": "yarn run eslint client --ext \".js,.jsx,.ts\"",
    "test": "yarn run build:test && yarn run lint",
    "test:client": "cd client && yarn run test",
    "build:test": "rm -rf public/webpack/test && RAILS_ENV=test NODE_ENV=test bin/webpack",
    "build:dev": "rm -rf public/webpack/development && RAILS_ENV=development NODE_ENV=development bin/webpack",
    "build:clean": "rm -rf public/webpack || true"
  },
  "dependencies": {
    "@babel/preset-react": "^7.10.4",
    "@rails/webpacker": "^5.1.1",
    "actioncable": "5.2.x",
    "autoprefixer": "^9.8.5",
    "axios": "^0.19.2",
    "bootstrap-loader": "^3.0.4",
    "bootstrap-sass": "^3.4.1",
    "classnames": "^2.2.6",
    "es5-shim": "^4.5.14",
    "estraverse": "^5.1.0",
    "expose-loader": "^1.0.0",
    "immutable": "^3.8.2",
    "imports-loader": "^1.1.0",
    "intl": "^1.2.5",
    "jquery": "^3.5.1",
    "jquery-ujs": "^1.2.2",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.19",
    "marked": "^1.1.1",
    "node-uuid": "^1.4.8",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-intl": "^5.4.1",
    "react-on-rails": "12.0.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "resolve-url-loader": "^3.1.1",
    "sass-resources-loader": "^2.0.3",
    "style-loader": "^1.2.1",
    "turbolinks": "^5.2.0",
    "url-loader": "^4.1.0",
    "webpack-merge": "^5.0.9"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.3.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "body-parser": "^1.19.0",
    "eslint": "^7.5.0",
    "eslint-config-shakacode": "^17.0.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "estraverse-fb": "^1.3.2",
    "express": "^4.17.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "preload-webpack-plugin": "^3.0.0-alpha.1",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "react-transform-hmr": "^1.0.4",
    "regenerator-runtime": "^0.13.7",
    "sleep": "^6.3.0",
    "webpack-dev-server": "^3.11.0"
  },
  "browser": {
    "fs": false
  },
  "jest": {
    "moduleNameMapper": {
      "\\.scss$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./app/libs/testHelper.js"
    ],
    "testRegex": "./app/.*.spec\\.jsx?$",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
